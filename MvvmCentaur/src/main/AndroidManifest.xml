<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.mobile.centaur">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" /> <!-- 震动权限 -->
    <uses-feature android:name="android.hardware.camera.autofocus" /> <!-- 自动聚焦权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />

    <application
        android:hardwareAccelerated="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:usesCleartextTraffic="true">
        android:allowBackup="true"
        android:label="@string/app_name">

        <activity
            android:name="com.mobile.centaur.h5container.ui.Bridge_WebViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pizidea.imagepicker.ui.activity.ImageCropActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pizidea.imagepicker.ui.activity.ImagesGridActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pizidea.imagepicker.ui.activity.ImagePreviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.google.zxing.activity.NGCCaptureActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.mobile.centaur.h5container.ui.DemoActivity"
            android:screenOrientation="portrait" />


    </application>

</manifest>
